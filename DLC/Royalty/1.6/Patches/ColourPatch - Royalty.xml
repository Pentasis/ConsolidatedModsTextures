<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Royalty</li>
		</mods>
		<match Class="XmlExtensions.UseSettings">
			<modId>cucumpear.stonetweaksvanilla</modId>
			<keys>
				<li>Col1</li>
				<li>Col2</li>
				<li>Col3</li>
				<li>Col4</li>
				<li>Col5</li>
			</keys>
			<defaultValues>
				<li>87,71,63</li>
				<li>75,84,92</li>
				<li>232,225,202</li>
				<li>64,64,64</li>
				<li>255,255,255</li>
			</defaultValues>
			<apply>
				<li Class="PatchOperationReplace">
				  <xpath>*/TerrainDef[defName = "FineTileSandstone"]/color</xpath>
				  <value><color>({Col1})</color></value>
				</li>
				<li Class="PatchOperationReplace">
				  <xpath>*/TerrainDef[defName = "FineTileGranite"]/color</xpath>
				  <value><color>({Col2})</color></value>
				</li>
				<li Class="PatchOperationReplace">
				  <xpath>*/TerrainDef[defName = "FineTileLimestone"]/color</xpath>
				  <value><color>({Col3})</color></value>
				</li>
				<li Class="PatchOperationReplace">
				  <xpath>*/TerrainDef[defName = "FineTileSlate"]/color</xpath>
				  <value><color>({Col4})</color></value>
				</li>
				<li Class="PatchOperationReplace">
				  <xpath>*/TerrainDef[defName = "FineTileMarble"]/color</xpath>
				  <value><color>({Col5})</color></value>
				</li>
			</apply>
		</match>
	</Operation>

</Patch>